    # Z "Скир?" id Z_d71fa34c
    Z "Skir?" id Z_d71fa34c

# game/event/hunger_zergling.rpy:480
translate italian S1_2dd1d813:

    # S1 "Как любопытно, ошибки быть не может. Это точно он!" id S1_2dd1d813
    S1 "Curiously enough, there could be no mistake. It's definitely him!" id S1_2dd1d813

# game/event/hunger_zergling.rpy:484
translate italian S1_975e973f:

    # S1 "Так даже еще лучше!" id S1_975e973f
    S1 "This is even better!" id S1_975e973f

# game/event/hunger_zergling.rpy:485
