    S "Even if I immobilize my body, it won't stop her..." id S_fd2ec4e0

# game/event/hunger_zergling.rpy:56
translate italian S_c5d24243:

    # S "Может... Да!" id S_c5d24243
    S "Maybe... Yes!" id S_c5d24243

